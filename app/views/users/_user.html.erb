<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Photo:</strong>
    <img src="<%= user.photo %>" height = 200  > 
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

</div>
